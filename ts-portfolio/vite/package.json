{
  "name": "vite-react-express-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server.js",
    "server:build": "babel --verbose --out-dir dist --only 'src/server.js,src/lib/' src",
    "client:build": "vite build",
    "build": "npm run server:build && npm run client:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@types/jquery": "^3.5.14",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "2.4.1",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vite": "^2.5.10"
  },
  "dependencies": {
    "@emailjs/browser": "^3.6.2",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.3",
    "@react-spring/parallax": "^9.4.5",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "dotenv": "^10.0.0",
    "emailjs": "^4.0.0",
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "pg": "^8.7.3",
    "postgresql": "^0.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-fast-marquee": "^1.3.2",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-router-dom": "^5.3.0",
    "typescript": "^4.7.4"
  }
}
